{
  "name": "pcr-exporter",
  "productName": "Priconne Exporter",
  "version": "0.0.1",
  "description": "A tool to parse, process, and export intercepted data from Princess Connect! Re:Dive",
  "main": "./app/main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w pcr-exporter",
    "pack": "webpack --mode=production",
    "dist": "electron-builder -w",
    "dev": "webpack --watch",
    "lint": "eslint . --ext .js"
  },
  "build": {
    "productName": "Priconne Exporter",
    "appId": "com.electron.pcr-exporter",
    "artifactName": "${productName}-${version}-${os}.${ext}",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "artifactName": "${productName}-Setup-${version}-${os}.${ext}"
    },
    "extraFiles": [
      {
        "from": "build/icon.ico",
        "to": "resources"
      }
    ]
  },
  "keywords": [],
  "author": "FabulousCupcake",
  "license": "Apache-2.0",
  "bugs": "https://github.com/FabulousCupcake/pcr-exporter/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/FabulousCupcake/pcr-exporter.git"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "electron": "^15.1.0",
    "electron-builder": "^22.11.7",
    "electron-rebuild": "^1.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-webpack-plugin": "^2.5.4",
    "prettier": "2.2.1",
    "prettier-eslint-cli": "^5.0.1",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "aes-js": "^3.1.2",
    "date-fns": "^2.23.0",
    "electron-json-storage": "^4.5.0",
    "electron-window-state": "^5.0.3",
    "fast-csv": "^4.3.6",
    "fs-extra": "^8.1.0",
    "http-mitm-proxy": "^0.9.0",
    "lodash": "^4.17.21",
    "mousetrap": "^1.6.5",
    "msgpack5": "^5.3.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.3.0",
    "request": "^2.88.2",
    "sanitize-filename": "^1.6.3",
    "semantic-ui-react": "^2.0.3",
    "uuid": "^3.4.0"
  }
}
